#include "lock.h"

/*
 * Lock deadbolt
 * returns 0 for success, 1 for fail
 */
int lock() {

}

/*
 * unlock deadbolt
 * returns 0 for success, 1 for fail
 */
int unlock() {

}

/*
 * calculates the value needed to set the specified frequency
 */
int freq(float f) {
	float v = ((1/((f)/750))*64000);
	return (int)v;
}

