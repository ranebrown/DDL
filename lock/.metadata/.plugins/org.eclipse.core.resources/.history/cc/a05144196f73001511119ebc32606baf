#include "driver_config.h"
#include "target_config.h"
#include "timer32.h"
#include "gpio.h"
#include "lock.h"

// frequency in kHz
#define f_48 1000
#define f_24 2000
#define f_12 4000
#define f_6 8000
#define f_3 16000
#define f_1 44000

int main() {
	GPIOInit();								// enable clock to gpio

	init_timer32PWM(1, f_48, MATCH0);		/* set timer32_1 to generate pwm signal with period, use MR0
											 * pwm signal on PIO1_1 (labeled as AD2)
											 */
	//setMatch_timer32PWM (1, 0, f_1/4);	// set the timer32_1 MR0 value
	enable_timer32(1);						// enable timer32_1


	return 0;
}
